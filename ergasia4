def f(s):
    s2= ''.join(str(ord(c)) for c in s)
    print(s2)
    s2=int(s2)
    prime=1;
    x=int((s2)**(1/2)) #αρκει να ελεγξουμε μεχρι την ριζα του αριθμου για διαιρετες
    if s2 > 1:
        for i in range(2, x):
            if (s2 % i) == 0:
              prime=0;
             
       

    if (prime==1):
        print ("prime number")
    else:
        print ("not prime number")
    

s2=input('give string')
f(s2)
